  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx prisma generate  && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:tsx": "tsx watch src/main.ts",
    "start:debug": "nest start --debug --watch",
    "seed": "node dist/prisma/seed.js",
    "prisma:run": "node dist/prisma/run.js",
    "migrate": "npx prisma migrate dev && npx prisma generate",
    "reset": "npx prisma migrate reset && yarn seed",
    "deploy": "npx prisma migrate deploy && npx prisma generate && yarn build && npx kill-port 3333 && pm2 start simplesst",
    "deploy:cd": "npx prisma migrate deploy && npx prisma generate && chmod +x ./scripts/start.sh && ./scripts/start.sh",
    "deploy:skip_build": "npx prisma migrate deploy && npx prisma generate && npx kill-port 3333 && pm2 start simplesst",
    "start:prod": "prisma migrate deploy && npx prisma generate && yarn build && node dist/src/main --max-old-space-size=16000",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env-cmd -f .env.test jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:start": "docker-compose up -d test_db && env-cmd -f .env.test npx prisma migrate dev && env-cmd -f .env.test yarn seed",
    "test:end": "docker-compose stop test_db && docker-compose rm -f test_db",
    "test:fast": "NODE_ENV=test env-cmd -f .env.test jest --config ./test/jest-e2e.json",
    "pretest:e2e": "docker-compose up -d test_db && env-cmd -f .env.test npx prisma migrate dev && env-cmd -f .env.test yarn seed",
    "test:e2e": "NODE_ENV=test env-cmd -f .env.test jest --config ./test/jest-e2e.json",
    "posttest:e2e": "docker-compose stop test_db && docker-compose rm -f test_db"
  },
